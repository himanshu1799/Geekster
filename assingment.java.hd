package April_07_2022;
import java.util.Scanner;
class assingment {

	public static void main(String[] args) {
		int n;
		int count=0;
		
		System.out.print("n");
		Scanner r=new Scanner(System.in);
		n=r.nextInt();
		
		for(int i=1; i<=n; i++)
		{
			if(n%i==0)
			
				count++;
			
		}
		if(count==2)
			System.out.print("Prime Number");
	    else
	    	System.out.print("Not Prime Number");

	}

}
